const WebSocket = require('ws')

const gemini = (currencyPair) =>{
  const ws = new WebSocket(`wss://api.gemini.com/v1/marketdata/${currencyPair}`)
  ws.on('message', function incoming(data) {
      console.log(data);
    });
}

let geminiBTCUSD = gemini('btcusd')
